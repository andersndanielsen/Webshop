@model IEnumerable<Model.Person>

@{
    ViewBag.Title = "Administrer Uregistrerte Brukere";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>Administrer Uregistrerte Brukere</h2>

<table class="adminTable">
    <tr class="tableHeaderRow">
        <th>
            Navn
        </th>
        <th>
            Tlf
        </th>
        <th>
            @Html.DisplayNameFor(model => model.address)
        </th>
        <th>
            Postnr
        </th>
        <th>
            Sted
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.firstName) @Html.DisplayFor(modelItem => item.surName)
        </td>
        <td>
        <td>
            @Html.DisplayFor(modelItem => item.telephoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.postcode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.postcodeArea)
        </td>
    </tr>
}

</table>
